<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
>

    <bean id="FileDataDao" class="com.kovka.dataaccess.dao.impl.FileDataDao">
        <property name="map" ref="FileDataMap"/>
    </bean>

    <bean id="SketchDao" class="com.kovka.dataaccess.dao.impl.SketchDao">
        <property name="map" ref="SketchMap"/>
    </bean>

    <bean id="SketchInfoDao" class="com.kovka.dataaccess.dao.impl.SketchInfoDao">
        <property name="map" ref="SketchInfoMap"/>
    </bean>

    <bean id="SketchProductDao" class="com.kovka.dataaccess.dao.impl.SketchProductDao">
        <property name="map" ref="SketchProductMap"/>
    </bean>

    <bean id="UserDao" class="com.kovka.dataaccess.dao.impl.UserDao">
        <property name="map" ref="UserMap"/>
    </bean>

    <bean id="AboutDao" class="com.kovka.dataaccess.dao.impl.AboutDao">
        <property name="map" ref="AboutMap"/>
    </bean>

    <bean id="AboutInfoDao" class="com.kovka.dataaccess.dao.impl.AboutInfoDao">
        <property name="map" ref="AboutInfoMap"/>
    </bean>

    <bean id="PriceListInfoDao" class="com.kovka.dataaccess.dao.impl.PriceListInfoDao">
        <property name="map" ref="PriceListInfoMap"/>
    </bean>

    <bean id="WorkSchemeInfoDao" class="com.kovka.dataaccess.dao.impl.WorkSchemeInfoDao">
        <property name="map" ref="WorkSchemeInfoMap"/>
    </bean>

</beans>